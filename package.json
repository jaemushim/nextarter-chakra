    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "next-sitemap": "^2.5.20",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.3"
